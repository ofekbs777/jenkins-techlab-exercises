pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                script {
                    def company = 'puzzle'
                    echo 'join the ${company}'
                    echo "join the ${company}"
                    echo '''join the ${company}'''
                    echo """join the ${company}"""

                    echo "tabulation>\t<"
                    echo "backspace>\b<"
                    echo "newline>\n<"
                    echo "carriage return>\r<"
                    echo "form feed>\f<"
                    echo "backslash>\\<"
                    echo "single quote>\'<"
                    echo "double quote>\"<"
                }
            }
        }
    }
}


















